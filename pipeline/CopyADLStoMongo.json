{
	"name": "CopyADLStoMongo",
	"properties": {
		"activities": [
			{
				"name": "Copy data from Mongo to ADLS",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "MongoDbAtlasSink",
						"writeBatchTimeout": "00:30:00",
						"writeBehavior": "insert"
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "ADLSJson",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "MongoDbAtlasMiriatech",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": [],
		"lastPublishTime": "2025-06-03T01:03:50Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}